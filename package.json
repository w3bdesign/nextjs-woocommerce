    "@types/react": "^19.0.10",
    "algoliasearch": "^4.24.0",
    "autoprefixer": "^10.4.20",
    "framer-motion": "12.4.7",
    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "next": "15.2.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-instantsearch-dom": "^6.40.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
