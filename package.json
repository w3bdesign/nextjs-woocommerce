    "lodash": "^4.17.21",
    "motion": "^12.23.24",
    "next": "16.0.6",
    "nprogress": "^0.2.0",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.67.0",
    "react-instantsearch-dom": "^6.40.4",
    "uuid": "^13.0.0",
