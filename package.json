    "next": "15.3.2",
    "nprogress": "^0.2.0",
    "postcss": "^8.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.56.4",
    "react-instantsearch-dom": "^6.40.4",
