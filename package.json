    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "babel-plugin-styled-components": "^2.1.4",   
    "eslint-config-next": "^15.2.0",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
